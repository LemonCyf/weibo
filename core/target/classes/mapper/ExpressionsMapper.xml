<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fifteen.dao.ExpressionsDao">
    <resultMap id="ExpressMap" type="com.fifteen.pojo.Expressions">
        <id property="expressionId" column="expressionId"/>
        <id property="expression" column="expression"/>
    </resultMap>
    <select id="findAllExpress" resultType="com.fifteen.pojo.Expressions">
         SELECT * FROM expressions
    </select>
</mapper>